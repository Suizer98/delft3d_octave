<?xml version="1.0"?>
<root>
  <title>Cyclone Parameters</title>
  <width>250</width>
  <height>140</height>
  <element>
    <style>edit</style>
    <position>90 100 110 20</position>
    <variable>handles.toolbox.tropicalcyclone.track.vmax(handles.toolbox.tropicalcyclone.activeCyclonePoint)</variable>
    <type>real</type>
    <text>Vmax (kts)</text>
    <textposition>left</textposition>
    <tooltipstring>Edit maximum wind speed (in knots)</tooltipstring>
  </element>
  <element>
    <style>edit</style>
    <position>90 75 110 20</position>
    <variable>handles.toolbox.tropicalcyclone.track.rmax(handles.toolbox.tropicalcyclone.activeCyclonePoint)</variable>
    <type>real</type>
    <text>Rmax (NM)</text>
    <textposition>left</textposition>
    <tooltipstring>Edit radius of maximum winds (in NM)</tooltipstring>
  </element>
  <element>
    <style>edit</style>
    <position>90 50 110 20</position>
    <variable>handles.toolbox.tropicalcyclone.track.pc(handles.toolbox.tropicalcyclone.activeCyclonePoint)</variable>
    <type>real</type>
    <text>Pc (hPa)</text>
    <textposition>left</textposition>
    <tooltipstring>Edit central pressure (in hPa)</tooltipstring>
  </element>
  <element>
    <style>pushok</style>
    <position>150 10 50 20</position>
  </element>
  <element>
    <style>pushcancel</style>
    <position>90 10 50 20</position>
  </element>
</root>
