<?xml version="1.0"?>
<root>
  <element>
    <style>panel</style>
    <text>Time Frame</text>
    <position>40 10 900 160</position>
  </element>
  <element>
    <style>edit</style>
    <position>130 125 120 20</position>
    <variable>handles.model.dflowfm.domain.refdate</variable>
    <type>date</type>
    <tooltipstring>Edit reference date</tooltipstring>
    <text>Reference Date</text>
    <textposition>left</textposition>
  </element>
  <element>
    <style>edit</style>
    <position>130 100 120 20</position>
    <variable>handles.model.dflowfm.domain.tstart</variable>
    <type>datetime</type>
    <tooltipstring>Edit model start time</tooltipstring>
    <text>Start Time</text>
    <textposition>left</textposition>
    <onchange>ddb_updateOutputTimesDelft3DFLOW</onchange>
  </element>
  <element>
    <style>edit</style>
    <position>130 75 120 20</position>
    <variable>handles.model.dflowfm.domain.tstop</variable>
    <type>datetime</type>
    <tooltipstring>Edit model stop time</tooltipstring>
    <text>Stop Time</text>
    <textposition>left</textposition>
    <onchange>ddb_updateOutputTimesDelft3DFLOW</onchange>
  </element>
  <element>
    <style>popupmenu</style>
    <text>Time Units</text>
    <position>350 125 120 20</position>
    <tooltipstring>Select time unit</tooltipstring>
    <variable>handles.model.dflowfm.domain.tunit</variable>
    <type>string</type>
    <listtext>Hours</listtext>
    <listtext>Minutes</listtext>
    <listtext>Seconds</listtext>
    <listvalue>H</listvalue>
    <listvalue>M</listvalue>
    <listvalue>S</listvalue>
  </element>
  <element>
    <style>edit</style>
    <position>350 100 120 20</position>
    <variable>handles.model.dflowfm.domain.dtuser</variable>
    <type>real</type>
    <tooltipstring>Edit user timestep in seconds (interval for external forcing update)</tooltipstring>
    <text>User Time Step</text>
  </element>
  <element>
    <style>edit</style>
    <position>350 75 120 20</position>
    <variable>handles.model.dflowfm.domain.dtmax</variable>
    <type>real</type>
    <tooltipstring>Edit maximum timestep in seconds</tooltipstring>
    <text>Max Time Step</text>
  </element>
  <element>
    <style>edit</style>
    <position>350 50 120 20</position>
    <variable>handles.model.dflowfm.domain.dtinit</variable>
    <type>real</type>
    <tooltipstring>Edit initial timestep in seconds</tooltipstring>
    <text>Initial Time Step</text>
  </element>
  <element>
    <style>checkbox</style>
    <position>350 25 120 20</position>
    <variable>handles.model.dflowfm.domain.autotimestep</variable>
    <tooltipstring>Use CFL timestep limit or not (1/0)</tooltipstring>
    <text>Auto Time Step</text>
  </element>
</root>
