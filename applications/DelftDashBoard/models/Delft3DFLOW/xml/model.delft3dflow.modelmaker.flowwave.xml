<?xml version="1.0"?>
<root>
  <title>Modelmaker FLOW+WAVE Options</title>
  <width>300</width>
  <height>175</height>
  <element>
    <style>edit</style>
    <position>125 125 50 20</position>
    <variable>coarseratio</variable>
    <type>real</type>
    <text>Ratio resolution flow to wave</text>
    <tooltipstring>Value 1 means same resolution. Value 2 means wave is 2 times as coarse</tooltipstring>
  </element>
  <element>
    <style>edit</style>
    <position>125 100 50 20</position>
    <variable>waveinterval</variable>
    <type>real</type>
    <text>Communication time [min]</text>
    <tooltipstring>Time step applied for wave updating.</tooltipstring>
  </element>
   <element>
     <style>edit</style>
     <position>125 75 50 20</position>
     <variable>windspeed</variable>
     <type>real</type>
     <text>Wind speed [m/s]</text>
     <tooltipstring>Wind speed applied.</tooltipstring>
  </element>
  <element>
    <style>edit</style>
    <position>125 50 50 20</position>
    <variable>winddirection</variable>
    <type>real</type>
    <text>Wind direction [degrees]</text>
    <tooltipstring>Wind direction applied.</tooltipstring>
  </element>
  <element>
    <style>pushok</style>
    <position>180 10 50 20</position>
  </element>
  <element>
    <style>pushcancel</style>
    <position>120 10 50 20</position>
  </element>
</root>
