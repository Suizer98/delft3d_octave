<?xml version="1.0"?>
<root>
  <element>
    <style>panel</style>
    <text>Physics</text>
    <position>40 10 900 160</position>
  </element>
  <element>
    <style>edit</style>
    <position>160 130 50 20</position>
    <text>Water Density (kg/m3)</text>
    <variable>handles.model.shorelines.domain.rhow</variable>
    <type>real</type>
  </element>
  <element>
    <style>popupmenu</style>
    <text>Phys Opts</text>
    <position>160 100 80 20</position>
    <type>string</type>
    <variable>handles.model.shorelines.domain.phys_opt</variable>
    <listtext>
      <variable>handles.model.shorelines.phys_opts_long</variable>
    </listtext>
    <listvalue>
      <variable>handles.model.shorelines.phys_opts_short</variable>
    </listvalue>
  </element>
  <element>
    <style>edit</style>
    <position>160 75 60 20</position>
    <text>Option 1</text>
    <variable>handles.model.shorelines.domain.option1_value</variable>
    <type>real</type>
    <tooltipstring>Edit value for phys opt 1</tooltipstring>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.phys_opt</variable>
        <value>opt1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>160 75 60 20</position>
    <text>Option 2</text>
    <variable>handles.model.shorelines.domain.option2_value</variable>
    <type>real</type>
    <tooltipstring>Edit value for phys opt 2</tooltipstring>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.phys_opt</variable>
        <value>opt2</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>160 75 60 20</position>
    <text>Option 3</text>
    <variable>handles.model.shorelines.domain.option3_value</variable>
    <type>real</type>
    <tooltipstring>Edit value for phys opt 3</tooltipstring>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.phys_opt</variable>
        <value>opt3</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>checkbox</style>
    <position>50 50 80 20</position>
    <text>Morphology</text>
    <variable>handles.model.shorelines.domain.morphology</variable>
    <type>boolean</type>
    <tooltipstring>Check to include morphology</tooltipstring>
  </element>
</root>
