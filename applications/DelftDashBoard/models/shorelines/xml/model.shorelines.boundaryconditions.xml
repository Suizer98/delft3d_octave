<?xml version="1.0"?>
<root>
  <element>
    <style>panel</style>
    <position>40 10 900 160</position>
    <text>Boundary Conditions</text>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>50 130 130 20</position>
    <text>Load Boundary File</text>
    <variable>handles.model.shorelines.domain.boundary.file</variable>
