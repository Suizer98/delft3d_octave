    <variable>handles.model.sfincs.domain(ad).input.rotation</variable>
    <type>real</type>
    <enable>0</enable>
  </element>
  <element>
    <style>edit</style>
    <position>90 30 60 20</position>
    <text>Latitude</text>
    <variable>handles.model.sfincs.domain(ad).input.latitude</variable>
    <type>real</type>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>260 130 130 20</position>
