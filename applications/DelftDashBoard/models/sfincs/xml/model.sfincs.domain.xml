    <style>popupmenu</style>
    <position>260 30 70 20</position>
    <text>Input Format</text>
    <variable>handles.model.sfincs.domain(ad).input.inputformat</variable>
    <type>string</type>
    <listtext>Binary</listtext>
    <listtext>ASCII</listtext>
    <listvalue>bin</listvalue>
    <listvalue>asc</listvalue>
  </element>
