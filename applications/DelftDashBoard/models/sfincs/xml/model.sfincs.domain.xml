    <selectiontext>Select depth file</selectiontext>
    <callback>ddb_sfincs_domain</callback>
    <option1>selectdepthfile</option1>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>260 105 130 20</position>
    <tooltipstring>Select index file</tooltipstring>
    <text>Index File</text>
    <variable>handles.model.sfincs.domain(ad).input.indexfile</variable>
    <extension>*.ind</extension>
    <selectiontext>Select index file</selectiontext>
    <callback>ddb_sfincs_domain</callback>
    <option1>selectindexfile</option1>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>260 80 130 20</position>
