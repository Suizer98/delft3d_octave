<?xml version="1.0"?>
<root>
  <element>
    <variable>activeparameter</variable>
    <type>integer</type>
    <style>popupmenu</style>
    <position>60 0 270 20</position>
    <listtext>
      <variable>parameternames</variable>
    </listtext>
    <callback>muppet_datasetGUI</callback>
    <option1>selectparameter</option1>
    <text>Parameter</text>
    <tooltipstring>Select parameter</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>selectedquantity</variable>
        <value>scalar</value>
        <operator>eq</operator>
      </check>
      <check>
        <variable>selectparameter</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
