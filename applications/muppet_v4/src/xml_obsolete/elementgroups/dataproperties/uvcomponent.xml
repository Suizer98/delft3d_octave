<?xml version="1.0"?>
<root>
  <element>
    <variable>ucomponent</variable>
    <type>string</type>
    <style>popupmenu</style>
    <position>60 0 120 20</position>
    <listtext>
      <variable>parameternames</variable>
    </listtext>
    <callback>muppet_datasetGUI</callback>
    <option1>selectucomponent</option1>
    <text>U</text>
    <tooltipstring>Select U component</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>quantity</variable>
        <value>vector2d</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <variable>vcomponent</variable>
    <type>string</type>
    <style>popupmenu</style>
    <position>210 0 120 20</position>
    <listtext>
      <variable>parameternames</variable>
    </listtext>
    <callback>muppet_datasetGUI</callback>
    <option1>selectvcomponent</option1>
    <text>V</text>
    <tooltipstring>Select V component</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>quantity</variable>
        <value>vector2d</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
