<?xml version="1.0"?>
<root>
  <element>
    <variable>tidalcomponent</variable>
    <type>string</type>
    <style>popupmenu</style>
    <position>60 0 270 20</position>
    <listtext>
      <variable>tidalcomponentlist</variable>
    </listtext>
    <callback>muppet_datasetGUI</callback>
    <option1>selecttidalcomponent</option1>
    <text>Component</text>
    <tooltipstring>Select tidal component</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>parameter</variable>
        <value>samples</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
