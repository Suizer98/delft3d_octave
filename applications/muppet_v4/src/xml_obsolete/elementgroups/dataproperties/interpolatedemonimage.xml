<?xml version="1.0"?>
<root>
  <element>
    <variable>interpolatedemonimage</variable>
    <type>integer</type>
    <style>checkbox</style>
    <position>10 0 270 20</position>
    <text>Interpolate DEM onto image grid</text>
    <tooltipstring>Check to interpolate DEM data onto image grid (if left unchecked, the image will be interpolated onto the DEM grid)</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>imagefile</variable>
        <value>isempty</value>
        <operator>ne</operator>
      </check>
      <check>
        <variable>georeferencefile</variable>
        <value>isempty</value>
        <operator>ne</operator>
      </check>
    </dependency>
  </element>
</root>
