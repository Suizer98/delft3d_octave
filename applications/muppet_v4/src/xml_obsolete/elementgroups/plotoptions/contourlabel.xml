<?xml version="1.0"?>
<root>
  <element>
    <style>panel</style>
    <position>0 0 350 40</position>
  </element>
  <element>
    <variable>contourlabels</variable>
    <type>boolean</type>
    <style>checkbox</style>
    <position>10 10 50 20</position>
    <text>Contour Labels</text>
  </element>
  <element>
    <variable>labels.font</variable>
    <style>selectfont</style>
    <position>120 10 50 20</position>
    <text>Font</text>
