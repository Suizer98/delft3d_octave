      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>contourlabels</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <variable>labels.spacing</variable>
    <type>real</type>
    <style>edit</style>
    <position>230 10 50 20</position>
    <text>Spacing</text>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>contourlabels</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
