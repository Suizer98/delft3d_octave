<?xml version="1.0"?>
<root>
  <title>Vector Legend</title>
  <width>220</width>
  <height>160</height>
  <tag>vectorlegend</tag>
  <element>
    <tag>editpositionx</tag>
    <style>edit</style>
    <position>130 120 60 20</position>
    <variable>position(1)</variable>
    <type>real</type>
    <text>Position X (cm)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editpositiony</tag>
    <style>edit</style>
    <position>130 90 60 20</position>
    <variable>position(2)</variable>
    <type>real</type>
    <text>Position Y (cm)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>selectfont</tag>
    <style>selectfont</style>
    <position>50 60 50 20</position>
    <text>Font</text>
    <variable>font</variable>
    <tooltipstring>Select legend font</tooltipstring>
  </element>
  <element>
    <tag>cancel</tag>
    <style>pushcancel</style>
    <position>50 20 60 20</position>
  </element>
  <element>
    <tag>ok</tag>
    <style>pushok</style>
    <position>120 20 70 20</position>
  </element>
</root>
