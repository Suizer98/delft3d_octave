<?xml version="1.0"?>
<root>
  <name>location1dxy</name>
  <longname>2D Polyline</longname>
  <plotoptionplottype>2d</plotoptionplottype>
  <plot>
    <type>map</type>
    <plotroutine>
      <name>plotpolyline</name>
      <plotoption>
        <name>plotroutine</name>
      </plotoption>
      <plotoption>
        <name>linegroup</name>
      </plotoption>
      <plotoption>
        <name>markergroup</name>
      </plotoption>
      <plotoption>
        <name>polygongroup</name>
      </plotoption>
      <plotoption>
        <name>legendtext</name>
      </plotoption>
    </plotroutine>
  </plot>
  <plot>
    <type>xy</type>
    <plotroutine>
      <name>plotpolyline</name>
      <plotoption>
        <name>plotroutine</name>
      </plotoption>
      <plotoption>
        <name>linecolor</name>
        <default>black</default>
        <default>red</default>
        <default>blue</default>
        <default>green</default>
      </plotoption>
      <plotoption>
        <name>linestyle</name>
        <default>-</default>
      </plotoption>
      <plotoption>
        <name>linewidth</name>
        <default>0.5</default>
      </plotoption>
      <plotoption>
        <name>marker</name>
        <default>none</default>
      </plotoption>
      <plotoption>
        <name>markersize</name>
        <default type='int'>10</default>
      </plotoption>
      <plotoption>
        <name>markeredgecolor</name>
        <default>black</default>
      </plotoption>
      <plotoption>
        <name>markerfacecolor</name>
        <default>black</default>
      </plotoption>
      <plotoption>
        <name>fillclosedpolygons</name>
        <default type='int'>0</default>
      </plotoption>
      <plotoption>
        <name>fillcolor</name>
        <default>lightgreen</default>
      </plotoption>
      <plotoption>
        <name>legendtext</name>
      </plotoption>
    </plotroutine>
  </plot>
</root>
