<?xml version="1.0"?>
<root>
  <dataproperty>
    <name>xcoordinate</name>
    <keyword>XCoordinate</keyword>
    <element>
      <style>popupmenu</style>
      <type>string</type>
      <text>X</text>
      <variable>xcoordinate</variable>
      <tooltipstring>Select X coordinate</tooltipstring>
      <callback>muppet_datasetGUI</callback>
      <option1>selectxcoordinate</option1>
      <position>60 0 270 20</position>
      <listtext>
        <variable>parameternames</variable>
      </listtext>
      <dependency>
        <action>visible</action>
        <checkfor>any</checkfor>
        <check>
          <variable>selectcoordinates</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
  </dataproperty>
</root>
