<?xml version="1.0"?>
<root>
  <plotoption>
    <name>surface3dgroup</name>
    <element>
      <style>panel</style>
      <text>Lighting</text>
      <position>0 0 350 230</position>
    </element>
    <element>
      <name>onecolor</name>
      <variable>onecolor</variable>
      <style>checkbox</style>
      <keyword>OneColor</keyword>
      <type>boolean</type>
      <default>0</default>
      <position>10 190 40 20</position>
      <check>
        <operator>ne</operator>
      </check>
      <text>One Color</text>
    </element>
    <element>
      <keyword>Color</keyword>
      <default>lightgreen</default>
      <style>selectcolor</style>
      <variable>color</variable>
      <includenone>no</includenone>
      <type>string</type>
      <text>Color</text>
      <tooltipstring>Select line color</tooltipstring>
      <position>130 190 100 20</position>
      <check>
        <variable>onecolor</variable>
        <operator>ne</operator>
        <value>0</value>
      </check>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>onecolor</variable>
          <operator>ne</operator>
          <value>0</value>
        </check>
      </dependency>
    </element>
    <element>
      <name>facelighting</name>
      <variable>facelighting</variable>
      <keyword>FaceLighting</keyword>
      <style>popupmenu</style>
      <listtext>none</listtext>
      <listtext>flat</listtext>
      <listtext>gouraud</listtext>
      <listtext>phong</listtext>
      <type>string</type>
      <text>Face Lighting</text>
      <position>110 160 80 20</position>
      <default>gouraud</default>
    </element>
    <element>
      <name>backfacelighting</name>
      <variable>backfacelighting</variable>
      <keyword>BackFaceLighting</keyword>
      <style>popupmenu</style>
      <listtext>unlit</listtext>
      <listtext>lit</listtext>
      <listtext>reverselit</listtext>
      <type>string</type>
      <text>Back Face Lighting</text>
      <position>110 130 80 20</position>
      <default>reverselit</default>
    </element>
    <element>
      <name>edgelighting</name>
      <variable>edgelighting</variable>
      <keyword>EdgeLighting</keyword>
      <style>popupmenu</style>
      <listtext>none</listtext>
      <listtext>flat</listtext>
      <listtext>gouraud</listtext>
      <listtext>phong</listtext>
      <type>string</type>
      <text>Edge Lighting</text>
      <position>110 100 80 20</position>
      <default>none</default>
    </element>
    <element>
      <name>ambientstrength</name>
      <variable>ambientstrength</variable>
      <keyword>AmbientStrength</keyword>
      <style>edit</style>
      <type>real</type>
      <text>Ambient Strength</text>
      <position>110 70 40 20</position>
      <default>0.5</default>
    </element>
    <element>
      <name>diffusestrength</name>
      <variable>diffusestrength</variable>
      <keyword>DiffuseStrength</keyword>
      <style>edit</style>
      <type>real</type>
      <text>Diffuse Strength</text>
      <position>300 70 40 20</position>
      <default>0.5</default>
    </element>
    <element>
      <name>specularstrength</name>
      <variable>specularstrength</variable>
      <keyword>SpecularStrength</keyword>
      <style>edit</style>
      <type>real</type>
      <text>Specular Strength</text>
      <position>110 40 40 20</position>
      <default>0.5</default>
    </element>
    <element>
      <name>specularexponent</name>
      <variable>specularexponent</variable>
      <keyword>SpecularExponent</keyword>
      <style>edit</style>
      <type>real</type>
      <text>Specular Exponent</text>
      <position>110 10 40 20</position>
      <default>100</default>
    </element>
    <element>
      <name>specularcolorreflectance</name>
      <variable>specularcolorreflectance</variable>
      <keyword>SpecularColorReflectance</keyword>
      <style>edit</style>
      <type>real</type>
      <text>Specular Color Reflectance</text>
      <position>300 10 40 20</position>
      <default>0.5</default>
    </element>
  </plotoption>
</root>
