<?xml version="1.0"?>
<root>
  <plotoption>
    <name>timebargroup</name>
    <element>
      <style>panel</style>
      <text>Time Bar</text>
      <position>0 0 350 140</position>
    </element>
    <element>
      <keyword>TimeBarType</keyword>
      <default>none</default>
      <check>
        <operator>ne</operator>
      </check>
      <variable>timebar.type</variable>
      <style>popupmenu</style>
      <position>40 100 60 20</position>
      <type>string</type>
      <listtext>none</listtext>
      <listtext>line</listtext>
      <listtext>marker</listtext>
    </element>
    <element>
      <keyword>TimeBarTime</keyword>
      <variable>timebar.time</variable>
      <text>Time</text>
      <check>
        <variable>timebar.type</variable>
        <operator>ne</operator>
        <value>none</value>
      </check>
      <style>edit</style>
      <position>150 100 120 20</position>
      <type>datetime</type>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <operator>ne</operator>
          <value>none</value>
        </check>
      </dependency>
    </element>
    <element>
      <keyword>TimeBarLineColor</keyword>
      <default>Red</default>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>line</value>
      </check>
      <check>
        <operator>ne</operator>
      </check>
      <style>selectcolor</style>
      <variable>timebar.linecolor</variable>
      <includenone>no</includenone>
      <type>string</type>
      <text>Color</text>
      <tooltipstring>Select line color</tooltipstring>
      <position>40 70 100 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>line</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <keyword>TimeBarLineWidth</keyword>
      <default>3</default>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>line</value>
      </check>
      <check>
        <operator>ne</operator>
      </check>
      <style>edit</style>
      <type>real</type>
      <variable>timebar.linewidth</variable>
      <text>Width</text>
      <position>190 70 50 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>line</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <keyword>TimeBarLineStyle</keyword>
      <default>-</default>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>line</value>
      </check>
      <check>
        <operator>ne</operator>
      </check>
      <style>selectlinestyle</style>
      <type>string</type>
      <variable>timebar.linestyle</variable>
      <text>Style</text>
      <position>280 70 60 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>line</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <name>timebarmarker</name>
      <keyword>TimeBarMarker</keyword>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>marker</value>
      </check>
      <default>o</default>
      <style>selectmarker</style>
      <type>string</type>
      <variable>timebar.marker</variable>
      <text>Type</text>
      <position>70 40 100 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>marker</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <name>timebarmarkersize</name>
      <keyword>TimeBarMarkerSize</keyword>
      <default>10</default>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>marker</value>
      </check>
      <style>selectmarkersize</style>
      <variable>timebar.markersize</variable>
      <type>int</type>
      <text>Size</text>
      <position>240 40 100 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>marker</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <name>timebarmarkeredgecolor</name>
      <keyword>TimeBarMarkerEdgeColor</keyword>
      <default>Red</default>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>marker</value>
      </check>
      <style>selectcolor</style>
      <variable>timebar.markeredgecolor</variable>
      <includenone>yes</includenone>
      <type>string</type>
      <text>Edge Color</text>
      <position>70 10 100 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>marker</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <name>timebarmarkerfacecolor</name>
      <keyword>TimeBarMarkerFaceColor</keyword>
      <default>Red</default>
      <check>
        <variable>timebar.type</variable>
        <operator>eq</operator>
        <value>marker</value>
      </check>
      <style>selectcolor</style>
      <variable>timebar.markerfacecolor</variable>
      <includenone>yes</includenone>
      <type>string</type>
      <text>Face Color</text>
      <position>240 10 100 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timebar.type</variable>
          <value>marker</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
  </plotoption>
</root>
