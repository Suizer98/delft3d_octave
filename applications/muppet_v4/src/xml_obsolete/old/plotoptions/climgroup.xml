<?xml version="1.0"?>
<root>
  <plotoption>
    <name>climgroup</name>
    <element>
      <name>clim</name>
      <variable>cmin cstep cmax</variable>
      <keyword>Contours</keyword>
      <check>
        <variable>usecustomcontours</variable>
        <value>0</value>
        <operator>eq</operator>
      </check>
      <type>real</type>
    </element>
    <element>
      <style>panel</style>
      <variable>adddatestring</variable>
      <text>Contours</text>
      <position>0 0 350 80</position>
    </element>
    <element>
      <style>edit</style>
      <variable>cmin</variable>
      <type>real</type>
      <text>C Min</text>
      <position>40 40 40 20</position>
      <check>
        <variable>usecustomcontours</variable>
        <value>0</value>
        <operator>eq</operator>
      </check>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>usecustomcontours</variable>
          <value>0</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <style>edit</style>
      <variable>cstep</variable>
      <type>real</type>
      <text>C Step</text>
      <position>125 40 40 20</position>
      <check>
        <variable>usecustomcontours</variable>
        <value>0</value>
        <operator>eq</operator>
      </check>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>usecustomcontours</variable>
          <value>0</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <style>edit</style>
      <variable>cmax</variable>
      <type>real</type>
      <text>C Max</text>
      <position>210 40 40 20</position>
      <check>
        <variable>usecustomcontours</variable>
        <value>0</value>
        <operator>eq</operator>
      </check>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>usecustomcontours</variable>
          <value>0</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <style>checkbox</style>
      <variable>usecustomcontours</variable>
      <type>boolean</type>
      <text>Custom Contours</text>
      <position>40 10 60 20</position>
    </element>
    <element>
      <style>pushbutton</style>
      <callback>muppet_editCustomContours</callback>
      <text>Edit ...</text>
      <position>150 10 70 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>usecustomcontours</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <variable>usecustomcontours</variable>
      <type>boolean</type>
      <keyword>UseCustomContours</keyword>
      <check>
        <variable>usecustomcontours</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </element>
    <element>
      <variable>customcontours</variable>
      <type>real</type>
      <keyword>CustomContours</keyword>
      <check>
        <variable>usecustomcontours</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </element>
  </plotoption>
</root>
