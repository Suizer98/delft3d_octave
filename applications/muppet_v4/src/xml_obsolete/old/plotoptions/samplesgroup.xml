<?xml version="1.0"?>
<root>
  <plotoption>
    <name>samplesgroup</name>
    <element>
      <style>panel</style>
      <text>Sample Properties</text>
      <position>0 0 350 80</position>
    </element>
    <element>
      <keyword>AddText</keyword>
      <default>1</default>
      <style>checkbox</style>
      <type>boolean</type>
      <variable>addtext</variable>
      <text>Add Text</text>
      <position>10 40 50 20</position>
    </element>
    <element>
      <name>textposition</name>
      <variable>textposition</variable>
      <keyword>TextPosition</keyword>
      <type>string</type>
      <default>northeast</default>
      <style>popupmenu</style>
      <position>150 40 90 20</position>
      <text>Position</text>
      <listtext>North West</listtext>
      <listtext>North</listtext>
      <listtext>North East</listtext>
      <listtext>West</listtext>
      <listtext>Middle</listtext>
      <listtext>East</listtext>
      <listtext>South West</listtext>
      <listtext>South</listtext>
      <listtext>South East</listtext>
      <listvalue>northwest</listvalue>
      <listvalue>north</listvalue>
      <listvalue>northeast</listvalue>
      <listvalue>west</listvalue>
      <listvalue>middle</listvalue>
      <listvalue>east</listvalue>
      <listvalue>southwest</listvalue>
      <listvalue>south</listvalue>
      <listvalue>southeast</listvalue>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>addtext</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <style>selectfont</style>
      <horizontalalignment>1</horizontalalignment>
      <verticalalignment>1</verticalalignment>
      <text>Font</text>
      <position>260 40 50 20</position>
      <variable>font</variable>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>addtext</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <name>fontname</name>
      <variable>font.name</variable>
      <keyword>FontName</keyword>
      <default>Helvetica</default>
    </element>
    <element>
      <name>fontsize</name>
      <variable>font.size</variable>
      <keyword>FontSize</keyword>
      <type>real</type>
      <default>8</default>
    </element>
      <element>
      <name>fontangle</name>
      <variable>font.angle</variable>
      <default>normal</default>
      <keyword>FontAngle</keyword>
    </element>
    <element>
      <name>fontweight</name>
      <variable>font.weight</variable>
      <keyword>FontWeight</keyword>
      <default>normal</default>
    </element>
    <element>
      <keyword>Decimals</keyword>
      <default>1</default>
      <tag>selectdecimals</tag>
      <style>popupmenu</style>
      <position>60 10 100 20</position>
      <variable>decimals</variable>
      <type>integer</type>
      <tooltipstring>Select number of decimals</tooltipstring>
      <text>Decimals</text>
      <listtext>automatic</listtext>
      <listtext>0</listtext>
      <listtext>1</listtext>
      <listtext>2</listtext>
      <listtext>3</listtext>
      <listtext>4</listtext>
      <listtext>5</listtext>
      <listtext>6</listtext>
      <listvalue>-1</listvalue>
      <listvalue>0</listvalue>
      <listvalue>1</listvalue>
      <listvalue>2</listvalue>
      <listvalue>3</listvalue>
      <listvalue>4</listvalue>
      <listvalue>5</listvalue>
      <listvalue>6</listvalue>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>addtext</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
  </plotoption>
</root>
