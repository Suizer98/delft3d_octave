<?xml version="1.0"?>
<root>
  <plotoption>
    <name>adddategroup</name>
    <element>
      <style>panel</style>
      <text>Date String</text>
      <position>0 0 350 110</position>
    </element>
    <element>
      <style>checkbox</style>
      <variable>adddatestring</variable>
      <type>boolean</type>
      <keyword>AddDate</keyword>
      <default>0</default>
      <check>
        <operator>ne</operator>
      </check>
      <text>Add Date</text>
      <position>50 70 50 20</position>
    </element>
    <element>
      <keyword>AddDatePosition</keyword>
      <default>upper-left</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
      <variable>adddate.position</variable>
      <style>popupmenu</style>
      <text>Position</text>
      <position>180 70 130 20</position>
      <type>string</type>
      <listtext>upper-left</listtext>
      <listtext>upper-right</listtext>
      <listtext>lower-left</listtext>
      <listtext>lower-right</listtext>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>adddatestring</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <keyword>AddDateFormat</keyword>
      <default>yyyy-mm-dd HH:MM:SS</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
      <variable>adddate.format</variable>
      <style>popupmenu</style>
      <text>Format</text>
      <type>string</type>
      <position>50 40 130 20</position>
      <listtext>
        <variable>mainhandles.dateformattexts</variable>
      </listtext>
      <listvalue>
        <variable>mainhandles.dateformats</variable>
      </listvalue>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>adddatestring</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <variable>adddate.font</variable>
      <style>selectfont</style>
      <text>Font</text>
      <position>200 40 50 20</position>
      <type>real</type>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>adddatestring</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <keyword>AddDatePrefix</keyword>
      <default>Date : </default>
      <check>
        <operator>notempty</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
      <variable>adddate.prefix</variable>
      <style>edit</style>
      <type>string</type>
      <text>Prefix</text>
      <position>50 10 120 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>adddatestring</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <keyword>AddDateSuffix</keyword>
      <default> </default>
      <check>
        <operator>notempty</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
      <variable>adddate.suffix</variable>
      <style>edit</style>
      <type>string</type>
      <text>Suffix</text>
      <position>220 10 120 20</position>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>adddatestring</variable>
          <value>1</value>
          <operator>eq</operator>
        </check>
      </dependency>
    </element>
    <element>
      <name>adddate.font.name</name>
      <keyword>AddDateFontName</keyword>
      <default>Helvetica</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
    </element>
    <element>
      <name>adddate.font.size</name>
      <keyword>AddDateFontSize</keyword>
      <type>real</type>
      <default>12</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
    </element>
    <element>
      <name>adddate.font.color</name>
      <keyword>AddDateFontColor</keyword>
      <default>Black</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
    </element>
    <element>
      <name>adddate.font.weight</name>
      <keyword>AddDateFontWeight</keyword>
      <default>normal</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
    </element>
    <element>
      <name>adddate.font.angle</name>
      <keyword>AddDateFontAngle</keyword>
      <default>normal</default>
      <check>
        <operator>ne</operator>
      </check>
      <check>
        <variable>adddatestring</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
    </element>
  </plotoption>
</root>
