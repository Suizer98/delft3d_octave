<?xml version="1.0"?>
<root>
  <title>Legend</title>
  <width>180</width>
  <height>420</height>
  <tag>legend</tag>
  <element>
    <tag>selectposition</tag>
    <style>popupmenu</style>
    <position>20 370 140 20</position>
    <variable>position</variable>
    <type>string</type>
    <text>Position</text>
    <textposition>above-left</textposition>
    <listtext>North</listtext>
    <listtext>South</listtext>
    <listtext>East</listtext>
    <listtext>West</listtext>
    <listtext>NorthEast</listtext>
    <listtext>NorthWest</listtext>
    <listtext>SouthEast</listtext>
    <listtext>SouthWest</listtext>
    <listtext>Best</listtext>
    <listtext>Custom</listtext>
  </element>
  <element>
    <tag>editpositionx</tag>
    <style>edit</style>
    <position>100 330 60 20</position>
    <variable>customposition(1)</variable>
    <type>real</type>
    <text>Position X (cm)</text>
    <textposition>left</textposition>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>position</variable>
        <value>Custom</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>editpositionx</tag>
    <style>edit</style>
    <position>100 300 60 20</position>
    <variable>customposition(2)</variable>
    <type>real</type>
    <text>Position Y (cm)</text>
    <textposition>left</textposition>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>position</variable>
        <value>Custom</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>editwidth</tag>
    <style>edit</style>
    <position>100 270 60 20</position>
    <variable>customposition(3)</variable>
    <type>real</type>
    <text>Width (cm)</text>
    <textposition>left</textposition>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>position</variable>
        <value>Custom</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>editheight</tag>
    <style>edit</style>
    <position>100 240 60 20</position>
    <variable>customposition(4)</variable>
    <type>real</type>
    <text>Height (cm)</text>
    <textposition>left</textposition>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>position</variable>
        <value>Custom</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>selectorientation</tag>
    <style>popupmenu</style>
    <position>20 180 140 20</position>
    <variable>orientation</variable>
    <type>string</type>
    <text>Orientation</text>
    <textposition>above-left</textposition>
    <listtext>Vertical</listtext>
    <listtext>Horizontal</listtext>
  </element>
  <element>
    <tag>toggleborder</tag>
    <style>checkbox</style>
    <position>20 150</position>
    <text>Border around legend</text>
    <variable>border</variable>
    <type>integer</type>
    <tooltipstring>Plot border around legend</tooltipstring>
  </element>
  <element>
    <tag>selectcolor</tag>
    <style>selectcolor</style>
    <position>20 100 150 20</position>
    <text>Color</text>
    <textposition>above-left</textposition>
    <variable>color</variable>
    <tooltipstring>Select legend background color</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>border</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>selectfont</tag>
    <style>selectfont</style>
    <position>20 60 50 20</position>
    <text>Font</text>
    <variable>font</variable>
    <tooltipstring>Select legend font</tooltipstring>
  </element>
  <element>
    <tag>cancel</tag>
    <style>pushcancel</style>
    <position>20 20 60 20</position>
  </element>
  <element>
    <tag>ok</tag>
    <style>pushok</style>
    <position>90 20 70 20</position>
  </element>
</root>
