<?xml version="1.0"?>
<root>
  <title>New Subplot</title>
  <width>220</width>
  <height>270</height>
  <tag>newsubplot</tag>
  <element>
    <tag>editpositionx</tag>
    <style>edit</style>
    <position>130 220 60 20</position>
    <variable>position(1)</variable>
    <type>real</type>
    <text>Position X (cm)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editpositionx</tag>
    <style>edit</style>
    <position>130 190 60 20</position>
    <variable>position(2)</variable>
    <type>real</type>
    <text>Position Y (cm)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editwidth</tag>
    <style>edit</style>
    <position>130 160 60 20</position>
    <variable>position(3)</variable>
    <type>real</type>
    <text>Width (cm)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editheight</tag>
    <style>edit</style>
    <position>130 130 60 20</position>
    <variable>position(4)</variable>
    <type>real</type>
    <text>Height (cm)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editname</tag>
    <style>edit</style>
    <position>60 90 130 20</position>
    <variable>name</variable>
    <type>string</type>
    <text>Name</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>cancel</tag>
    <style>pushcancel</style>
    <position>40 30 70 20</position>
  </element>
  <element>
    <tag>ok</tag>
    <style>pushok</style>
    <position>120 30 70 20</position>
  </element>
</root>
