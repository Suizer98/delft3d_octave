<?xml version="1.0"?>
<root>
  <title>New Dataset from URL</title>
  <width>1000</width>
  <height>270</height>
  <tag>newurl</tag>
  <element>
    <tag>url</tag>
    <style>edit</style>
    <position>130 220 740 20</position>
    <variable>url</variable>
    <type>string</type>
    <text>Data URL</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>polygon</tag>
    <style>edit</style>
    <position>130 190 740 20</position>
    <variable>polygonfile</variable>
    <type>string</type>
    <text>Polygon file</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>cancel</tag>
    <style>pushcancel</style>
    <position>40 30 70 20</position>
  </element>
  <element>
    <tag>ok</tag>
    <style>pushok</style>
    <position>120 30 70 20</position>
  </element>
</root>
