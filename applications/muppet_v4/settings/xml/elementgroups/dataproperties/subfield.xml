<?xml version="1.0"?>
<root>
  <element>
    <variable>subfieldnumber</variable>
    <type>integer</type>
    <style>popupmenu</style>
    <position>60 0 270 20</position>
    <listtext>
      <variable>subfields</variable>
    </listtext>
    <callback>muppet_datasetGUI</callback>
    <option1>selectsubfield</option1>
    <text>Subfield</text>
    <tooltipstring>Select subfield</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>nrsubfields</variable>
        <value>0</value>
        <operator>gt</operator>
      </check>
    </dependency>
  </element>
</root>
