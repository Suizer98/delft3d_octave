<?xml version="1.0"?>
<root>
  <element>
    <variable>component</variable>
    <type>string</type>
    <style>popupmenu</style>
    <position>60 0 270 20</position>
    <listtext>vector</listtext>
    <listtext>vector (split x,y)</listtext>
    <listtext>vector (split m,n)</listtext>
    <listtext>magnitude</listtext>
    <listtext>angle (radians)</listtext>
    <listtext>angle (degrees)</listtext>
    <listtext>x component</listtext>
    <listtext>y component</listtext>
    <listtext>m component</listtext>
    <listtext>n component</listtext>
    <callback>muppet_datasetGUI</callback>
    <option1>selectcomponent</option1>
    <text>Component</text>
    <tooltipstring>Select component</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>quantity</variable>
        <value>vector2d</value>
        <operator>eq</operator>
      </check>
      <check>
        <variable>quantity</variable>
        <value>vector3d</value>
        <operator>eq</operator>
      </check>
      <check>
        <variable>selectedquantity</variable>
        <value>vector2d</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
