%% Get the names of the domains
[Success,Domains] = qp_getdata(FI,'domains');
if isempty(Domains)
    Domains = {'<unnamed location>'};
end

%% Define Locations structure
Locations = [];
for i = 1:length(Domains)
    Locations = addlocation(Locations,Domains{i},'Struct2D+', ...
        {'M' 'MI' 'WholeCells' 'N' 'NI' 'WholeCells' 'K' 'KI' 'WholeCells'},[],i,[],[],[]);
end
